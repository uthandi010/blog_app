{"ast":null,"code":"import { mapGetters } from 'vuex';\nimport axios from 'axios';\nexport default {\n  name: 'UserProfile',\n  data() {\n    return {\n      user: null,\n      blogPosts: []\n    };\n  },\n  computed: {\n    ...mapGetters('auth', ['getUser']),\n    userToken() {\n      return this.getUser.user_token;\n    },\n    current_user() {\n      return this.getUser.current_user;\n    }\n  },\n  async created() {\n    const userId = this.$route.params.id;\n    try {\n      const userResponse = await axios.get(`http://127.0.0.1:3000/api/v1/profiles/${userId}`, {\n        headers: {\n          'Authorization': `Bearer ${this.userToken}`\n        }\n      });\n      this.user = userResponse.data.user;\n      this.blogPosts = userResponse.data.posts;\n      console.log('User data:', this.user);\n    } catch (error) {\n      console.error('Error fetching user data:', error);\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","axios","name","data","user","blogPosts","computed","userToken","getUser","user_token","current_user","created","userId","$route","params","id","userResponse","get","headers","posts","console","log","error"],"sources":["/Users/francium/app/frontend/src/components/users/UserProfile.vue"],"sourcesContent":["<template>\n    <div class=\"user-profile\">\n        <h1>{{ user.firstname }} {{ user.lastname }}</h1>\n        <h2>{{ user.email }}</h2>\n        <div class=\"page-wrapper\">\n            <div class=\"blog-container\">\n                <div class=\"blog-card\" v-for=\"(post, index) in blogPosts\" :key=\"index\">\n                    <img :src=\"post.image_url\" :alt=\"post.title\" class=\"blog-image\">\n                    <div class=\"blog-content\">\n                        <div class=\"content-row\">\n                            <span class=\"label\">Title</span>\n                            <h3 class=\"blog-title\">{{ post.title }}</h3>\n                        </div>\n                        <div class=\"content-row\">\n                            <span class=\"label\">Author</span>\n                            <p class=\"blog-author\">{{post.user.firstname }} {{ post.user.lastname }}</p>\n                        </div>\n                        <div class=\"content-row\">\n                            <span class=\"label\">Description</span>\n                            <p class=\"blog-description\">{{ post.description }}</p>\n                        </div>\n                        <div class=\"content-row\">\n                            <router-link :to=\"`/blogpost/${post.id}`\" class=\"nav-button auth-button\">View</router-link>\n                        </div>\n                    </div>\n            </div>\n        </div>\n    </div>\n    </div>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex';\nimport axios from 'axios';\n\nexport default {\n    name: 'UserProfile',\n    data() {\n        return {\n            user: null,\n            blogPosts: []\n        };\n    },\n    computed: {\n        ...mapGetters('auth', ['getUser']),\n        userToken() {\n            return this.getUser.user_token;\n        },\n        current_user() {\n            return this.getUser.current_user;\n        }\n    },\n    async created() {\n        const userId = this.$route.params.id;\n        try {\n            const userResponse = await axios.get(`http://127.0.0.1:3000/api/v1/profiles/${userId}`,\n            {\n                headers: {\n                    'Authorization': `Bearer ${this.userToken}`\n                }\n            });\n            this.user = userResponse.data.user;\n            this.blogPosts = userResponse.data.posts;\n            console.log('User data:', this.user);\n        } catch (error) {\n            console.error('Error fetching user data:', error);\n        }\n    }\n};\n</script>\n\n<style scoped>\n.user-profile {\n    padding: 20px;\n}\n\n.user-profile h1,\n.user-profile h2 {\n    margin-bottom: 10px;\n}\n\n.user-profile p {\n    margin: 5px 0;\n}\n\n.user-profile ul {\n    list-style-type: none;\n    padding: 0;\n}\n\n.user-profile li {\n    margin-bottom: 20px;\n}\n</style>"],"mappings":"AAgCA,SAASA,UAAS,QAAS,MAAM;AACjC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE,IAAI;MACVC,SAAS,EAAE;IACf,CAAC;EACL,CAAC;EACDC,QAAQ,EAAE;IACN,GAAGN,UAAU,CAAC,MAAM,EAAE,CAAC,SAAS,CAAC,CAAC;IAClCO,SAASA,CAAA,EAAG;MACR,OAAO,IAAI,CAACC,OAAO,CAACC,UAAU;IAClC,CAAC;IACDC,YAAYA,CAAA,EAAG;MACX,OAAO,IAAI,CAACF,OAAO,CAACE,YAAY;IACpC;EACJ,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACZ,MAAMC,MAAK,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE;IACpC,IAAI;MACA,MAAMC,YAAW,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAAC,yCAAyCL,MAAM,EAAE,EACtF;QACIM,OAAO,EAAE;UACL,eAAe,EAAE,UAAU,IAAI,CAACX,SAAS;QAC7C;MACJ,CAAC,CAAC;MACF,IAAI,CAACH,IAAG,GAAIY,YAAY,CAACb,IAAI,CAACC,IAAI;MAClC,IAAI,CAACC,SAAQ,GAAIW,YAAY,CAACb,IAAI,CAACgB,KAAK;MACxCC,OAAO,CAACC,GAAG,CAAC,YAAY,EAAE,IAAI,CAACjB,IAAI,CAAC;IACxC,EAAE,OAAOkB,KAAK,EAAE;MACZF,OAAO,CAACE,KAAK,CAAC,2BAA2B,EAAEA,KAAK,CAAC;IACrD;EACJ;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
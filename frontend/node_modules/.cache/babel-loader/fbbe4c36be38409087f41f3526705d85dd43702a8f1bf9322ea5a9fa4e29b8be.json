nt {\n  padding: 20px;\n  background: linear-gradient(135deg, #c2ecd9 0%, #a8e6c9 100%);\n  flex-grow: 1;\n  display: flex;\n  flex-direction: column;\n  gap: 15px;\n}\n\n.content-row {\n  display: flex;\n  align-items: flex-start;\n  gap: 12px;\n}\n\n.label {\n  min-width: 90px;\n  font-weight: 600;\n  color: #2c3e50;\n  background-color: rgba(255, 255, 255, 0.7);\n  padding: 6px 12px;\n  border-radius: 6px;\n  font-size: 0.9em;\n  text-align: center;\n}\n\n.blog-title {\n  font-size: 1.3em;\n  margin: 0;\n  color: #2c3e50;\n  font-weight: 600;\n  flex: 1;\n  line-height: 1.4;\n}\n\n.blog-author {\n  font-size: 1em;\n  color: #2c3e50;\n  margin: 0;\n  flex: 1;\n  line-height: 1.8;\n}\n\n.blog-description {\n  font-size: 1em;\n  color: #2c3e50;\n  margin: 0;\n  flex: 1;\n  line-height: 1.6;\n}\n\n@media (max-width: 768px) {\n  .blog-container {\n    grid-template-columns: 1fr;\n    padding: 15px;\n  }\n\n  .content-row {\n    flex-direction: column;\n    gap: 8px;\n  }\n\n  .label {\n    min-width: auto;\n    align-self: flex-start;\n  }\n}\n</style>"],"mappings":";;EACOA,KAAK,EAAC;AAAc;;EAClBA,KAAK,EAAC;AAAgB;mBAF/B;;EAKaA,KAAK,EAAC;AAAc;;EAClBA,KAAK,EAAC;AAAa;;EAElBA,KAAK,EAAC;AAAY;;EAEnBA,KAAK,EAAC;AAAa;;EAEnBA,KAAK,EAAC;AAAa;;EAEnBA,KAAK,EAAC;AAAa;;EAEnBA,KAAK,EAAC;AAAkB;;EAKxBA,KAAK,EAAC;AAAa;;EAEnBA,KAAK,EAAC;AAAkB;;uBAtBrCC,mBAAA,CA2BM,OA3BNC,UA2BM,GA1BJC,mBAAA,CAyBM,OAzBNC,UAyBM,I,kBAxBJH,mBAAA,CAuBMI,SAAA,QA1BZC,WAAA,CAGqDC,KAAA,CAAAC,SAAS,EAH9D,CAGqCC,IAAI,EAAEC,KAAK;yBAA1CT,mBAAA,CAuBM;MAvBDD,KAAK,EAAC,WAAW;MAAqCW,GAAG,EAAED;QAC9DP,mBAAA,CAAgE;MAA1DS,GAAG,EAAEH,IAAI,CAACI,SAAS;MAAGC,GAAG,EAAEL,IAAI,CAACM,KAAK;MAAEf,KAAK,EAAC;4BAJ3DgB,UAAA,GAKQb,mBAAA,CAoBM,OApBNc,UAoBM,GAnBJd,mBAAA,CAGM,OAHNe,UAGM,G,0BAFJf,mBAAA,CAAgC;MAA1BH,KAAK,EAAC;IAAO,GAAC,OAAK,sBACzBG,mBAAA,CAA4C,MAA5CgB,UAA4C,EAAAC,gBAAA,CAAlBX,IAAI,CAACM,KAAK,iB,GAEtCZ,mBAAA,CAGM,OAHNkB,UAGM,G,0BAFJlB,mBAAA,CAAiC;MAA3BH,KAAK,EAAC;IAAO,GAAC,QAAM,sBAC1BG,mBAAA,CAA4E,KAA5EmB,UAA4E,EAAAF,gBAAA,CAAnDX,
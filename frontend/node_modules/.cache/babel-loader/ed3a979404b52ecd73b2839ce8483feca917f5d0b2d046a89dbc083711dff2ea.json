{"ast":null,"code":"import { mapGetters } from 'vuex';\nimport axios from 'axios';\nexport default {\n  name: 'UserProfile',\n  data() {\n    return {\n      user: null,\n      blogPosts: []\n    };\n  },\n  computed: {\n    ...mapGetters('auth', ['getUser']),\n    userToken() {\n      return this.getUser.user_token;\n    },\n    current_user() {\n      return this.getUser.current_user;\n    }\n  },\n  async created() {\n    const userId = this.$route.params.id;\n    try {\n      const userResponse = await axios.get(`http://127.0.0.1:3000/api/v1/profiles/${userId}`, {\n        headers: {\n          'Authorization': `Bearer ${this.userToken}`\n        }\n      });\n      this.user = userResponse.data.user;\n      console.log('User data:', this.user);\n    } catch (error) {\n      console.error('Error fetching user data:', error);\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","axios","name","data","user","blogPosts","computed","userToken","getUser","user_token","current_user","created","userId","$route","params","id","userResponse","get","headers","console","log","error"],"sources":["/Users/francium/app/frontend/src/components/users/UserProfile.vue"],"sourcesContent":["<template>\n    <div class=\"user-profile\">\n        <h1>{{ user.firstname }} {{ user.lastname }}</h1>\n        <h2>{{ user.email }}</h2>\n        \n        <ul>\n            <li v-for=\"post in blogPosts\" :key=\"post.id\">\n                <h3>{{ post.title }}</h3>\n                <p>{{ post.content }}</p>\n            </li>\n        </ul>\n    </div>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex';\nimport axios from 'axios';\n\nexport default {\n    name: 'UserProfile',\n    data() {\n        return {\n            user: null,\n            blogPosts: []\n        };\n    },\n    computed: {\n        ...mapGetters('auth', ['getUser']),\n        userToken() {\n            return this.getUser.user_token;\n        },\n        current_user() {\n            return this.getUser.current_user;\n        }\n    },\n    async created() {\n        const userId = this.$route.params.id;\n        try {\n            const userResponse = await axios.get(`http://127.0.0.1:3000/api/v1/profiles/${userId}`,\n            {\n                headers: {\n                    'Authorization': `Bearer ${this.userToken}`\n                }\n            });\n            this.user = userResponse.data.user;\n            console.log('User data:', this.user);\n        } catch (error) {\n            console.error('Error fetching user data:', error);\n        }\n    }\n};\n</script>\n\n<style scoped>\n.user-profile {\n    padding: 20px;\n}\n\n.user-profile h1,\n.user-profile h2 {\n    margin-bottom: 10px;\n}\n\n.user-profile p {\n    margin: 5px 0;\n}\n\n.user-profile ul {\n    list-style-type: none;\n    padding: 0;\n}\n\n.user-profile li {\n    margin-bottom: 20px;\n}\n</style>"],"mappings":"AAeA,SAASA,UAAS,QAAS,MAAM;AACjC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE,IAAI;MACVC,SAAS,EAAE;IACf,CAAC;EACL,CAAC;EACDC,QAAQ,EAAE;IACN,GAAGN,UAAU,CAAC,MAAM,EAAE,CAAC,SAAS,CAAC,CAAC;IAClCO,SAASA,CAAA,EAAG;MACR,OAAO,IAAI,CAACC,OAAO,CAACC,UAAU;IAClC,CAAC;IACDC,YAAYA,CAAA,EAAG;MACX,OAAO,IAAI,CAACF,OAAO,CAACE,YAAY;IACpC;EACJ,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACZ,MAAMC,MAAK,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE;IACpC,IAAI;MACA,MAAMC,YAAW,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAAC,yCAAyCL,MAAM,EAAE,EACtF;QACIM,OAAO,EAAE;UACL,eAAe,EAAE,UAAU,IAAI,CAACX,SAAS;QAC7C;MACJ,CAAC,CAAC;MACF,IAAI,CAACH,IAAG,GAAIY,YAAY,CAACb,IAAI,CAACC,IAAI;MAClCe,OAAO,CAACC,GAAG,CAAC,YAAY,EAAE,IAAI,CAAChB,IAAI,CAAC;IACxC,EAAE,OAAOiB,KAAK,EAAE;MACZF,OAAO,CAACE,KAAK,CAAC,2BAA2B,EAAEA,KAAK,CAAC;IACrD;EACJ;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}